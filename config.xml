<?xml version='1.0' encoding='utf-8'?>
<widget id="com.Cbtis.Jelly J." version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Recetario</name>
    <description>
        Usar en horizontal para un mejor funcionamiento de la APP.
    </description>
    <author email="Ulisespadflo@gmail.com">
        Squigly team
    </author>
 

    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
